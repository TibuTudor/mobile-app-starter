# Docker Compose environment variables
# Copy this file to .env and update the values.

# Your machine's LAN IP address (e.g. 192.168.1.42)
# Find it with: ipconfig getifaddr en0 (macOS) or hostname -I (Linux)
HOST_IP=192.168.1.X

# MySQL credentials
DB_DATABASE=laravel
DB_USERNAME=laravel
DB_PASSWORD=secret
DB_ROOT_PASSWORD=rootsecret

# Laravel APP_KEY (leave blank â€” generated automatically on first run)
APP_KEY=
